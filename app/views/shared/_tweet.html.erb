<%= link_to tweet.title, tweet_path(tweet.id) %>
<%= link_to tweet.text, tweet_path(tweet.id) %>
<%= link_to tweet.prefecture.prefecture, prefecture_list_tweet_path(tweet.prefecture_id) %>
